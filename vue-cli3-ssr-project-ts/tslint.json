{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [true,"spaces",2],
    "quotemark": false,
    "semicolon": false,
    "arrow-parens": false,
    "space-before-function-paren": false,
    "eofline": false,
    "trailing-comma": false,
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false
  }
}